{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "The AWS CloudFormation template for this Serverless application",
  "Resources": {
    "ServerlessDeploymentBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketEncryption": {
          "ServerSideEncryptionConfiguration": [
            {
              "ServerSideEncryptionByDefault": {
                "SSEAlgorithm": "AES256"
              }
            }
          ]
        }
      }
    },
    "ServerlessDeploymentBucketPolicy": {
      "Type": "AWS::S3::BucketPolicy",
      "Properties": {
        "Bucket": {
          "Ref": "ServerlessDeploymentBucket"
        },
        "PolicyDocument": {
          "Statement": [
            {
              "Action": "s3:*",
              "Effect": "Deny",
              "Principal": "*",
              "Resource": [
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      },
                      "/*"
                    ]
                  ]
                },
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      }
                    ]
                  ]
                }
              ],
              "Condition": {
                "Bool": {
                  "aws:SecureTransport": false
                }
              }
            }
          ]
        }
      }
    },
    "SeleniumLambdaLayer": {
      "Type": "AWS::Lambda::LayerVersion",
      "Properties": {
        "Content": {
          "S3Bucket": {
            "Ref": "ServerlessDeploymentBucket"
          },
          "S3Key": "serverless/selenium-layer/dev/1651753736088-2022-05-05T12:28:56.088Z/selenium.zip"
        },
        "LayerName": "selenium"
      }
    },
    "ChromedriverLambdaLayer": {
      "Type": "AWS::Lambda::LayerVersion",
      "Properties": {
        "Content": {
          "S3Bucket": {
            "Ref": "ServerlessDeploymentBucket"
          },
          "S3Key": "serverless/selenium-layer/dev/1651753736088-2022-05-05T12:28:56.088Z/chromedriver.zip"
        },
        "LayerName": "chromedriver",
        "Description": "chrome driver layer"
      }
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": {
        "Ref": "ServerlessDeploymentBucket"
      },
      "Export": {
        "Name": "sls-selenium-layer-dev-ServerlessDeploymentBucketName"
      }
    },
    "SeleniumLambdaLayerQualifiedArn": {
      "Description": "Current Lambda layer version",
      "Value": {
        "Ref": "SeleniumLambdaLayer"
      },
      "Export": {
        "Name": "sls-selenium-layer-dev-SeleniumLambdaLayerQualifiedArn"
      }
    },
    "SeleniumLambdaLayerHash": {
      "Description": "Current Lambda layer hash",
      "Value": "3cf23b3661aadf165b13dda517bc38d23e9c316f",
      "Export": {
        "Name": "sls-selenium-layer-dev-SeleniumLambdaLayerHash"
      }
    },
    "SeleniumLambdaLayerS3Key": {
      "Description": "Current Lambda layer S3Key",
      "Value": "serverless/selenium-layer/dev/1651753736088-2022-05-05T12:28:56.088Z/selenium.zip",
      "Export": {
        "Name": "sls-selenium-layer-dev-SeleniumLambdaLayerS3Key"
      }
    },
    "ChromedriverLambdaLayerQualifiedArn": {
      "Description": "Current Lambda layer version",
      "Value": {
        "Ref": "ChromedriverLambdaLayer"
      },
      "Export": {
        "Name": "sls-selenium-layer-dev-ChromedriverLambdaLayerQualifiedArn"
      }
    },
    "ChromedriverLambdaLayerHash": {
      "Description": "Current Lambda layer hash",
      "Value": "2781871e54e095a22aba5ebdbdd302644c581a8a",
      "Export": {
        "Name": "sls-selenium-layer-dev-ChromedriverLambdaLayerHash"
      }
    },
    "ChromedriverLambdaLayerS3Key": {
      "Description": "Current Lambda layer S3Key",
      "Value": "serverless/selenium-layer/dev/1651753736088-2022-05-05T12:28:56.088Z/chromedriver.zip",
      "Export": {
        "Name": "sls-selenium-layer-dev-ChromedriverLambdaLayerS3Key"
      }
    },
    "SeleniumLayerExport": {
      "Value": {
        "Ref": "SeleniumLambdaLayer"
      },
      "Export": {
        "Name": "SeleniumLambdaLayer"
      }
    },
    "ChromedriverLayerExport": {
      "Value": {
        "Ref": "ChromedriverLambdaLayer"
      },
      "Export": {
        "Name": "ChromedriverLambdaLayer"
      }
    }
  }
}